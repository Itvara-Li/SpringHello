<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING/DTD BEAN/EN" 
"http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
	<bean id="helloWorld" class="com.coffee.spring.hello.HelloWorld"></bean>
	<bean id="helloChina" class="com.coffee.spring.hello.HelloChina"></bean>
	<bean id="person" class="com.coffee.spring.hello.Person">
	    <property name="helloMessage" ref="helloChina"/>
	</bean>
</beans>